<?xml version="1.0" encoding="utf-8" ?>
<Defs>

    <RecipeDef ParentName="SurgeryFlesh">
        <defName>InstallArtificialBrain</defName>
        <label>install artificial brain</label>
        <description>Install a artificial brain for mech.</description>
        <workerClass>Recipe_InstallArtificialBodyPart</workerClass>
        <jobString>Installing artificial brain.</jobString>
        <workAmount>2000</workAmount>
        <skillRequirements>
            <Medicine>5</Medicine>
        </skillRequirements>
        <ingredients>
            <li>
                <filter>
                    <categories>
                        <li>Medicine</li>
                    </categories>
                </filter>
                <count>2</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>ArtificialBrain</li>
                    </thingDefs>
                </filter>
                <count>1</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <categories>
                <li>Medicine</li>
            </categories>
            <thingDefs>
                <li>ArtificialBrain</li>
            </thingDefs>
        </fixedIngredientFilter>
        <appliedOnFixedBodyParts>
            <li>ArtificialBrain</li>
        </appliedOnFixedBodyParts>
        <addsHediff>ArtificialBrain</addsHediff>
    </RecipeDef>

